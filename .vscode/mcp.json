{
	"servers": {
		"supabase": {
			"command": "cmd",
			"args": [
				"/c",
				"npx",
				"-y",
				"@supabase/mcp-server-supabase@latest",
				"--project-ref=oxrakwvbpudnzspmxkng"
			],
			"env": {
				"SUPABASE_ACCESS_TOKEN": "sbp_e97042036c4d174a410e02428c058602c6d9d1ca"
			},
			"type": "stdio"
		},
		"youtube": {
			"command": "npx",
			"args": [
				"-y",
				"@sfiorini/youtube-mcp"
			],
			"env": {
				"YOUTUBE_API_KEY": "${input:apiKey}"
			},
			"type": "stdio"
		}
	},
	"inputs": [
		{
			"type": "promptString",
			"id": "supabase-access-token",
			"description": "Supabase personal access token",
			"password": true
		},
		{
			"type": "promptString",
			"id": "apiKey",
			"description": "YouTube API Key",
			"password": true
		}
	]
}